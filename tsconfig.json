{
  "compilerOptions": {
    "target": "ESNext",
    "sourceMap": false,
    "outDir": "./dist",
    "module": "ES6",
    "jsx": "react",
    "noImplicitAny": true,
    "allowSyntheticDefaultImports": true,
    "moduleResolution": "Node",
    "baseUrl": "./",
    "paths": {
      "@types": ["src/interface"],
      "@store": ["src/store"],
      "@util": ["src/common/util"],
    }
  },
  "include": ["src/**/*"]
}
